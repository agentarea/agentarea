# OIDC Configuration for JWT Authentication
# Replace these values with your actual OIDC provider configuration

# JWKS URI for token verification (required for OIDC)
OIDC_JWKS_URI=https://your-oidc-provider.com/.well-known/jwks.json

# JWT algorithm for token verification (default: RS256)
OIDC_ALGORITHM=RS256

# OIDC issuer URL (optional, for additional validation)
OIDC_ISSUER=https://your-oidc-provider.com

# OIDC audience (optional, for additional validation)
OIDC_AUDIENCE=your-api-audience

# JWT Secret Key (for local development/testing)
JWT_SECRET_KEY=your-secret-key-change-in-production

# JWT Algorithm (for local development/testing)
JWT_ALGORITHM=HS256

# JWT Access Token Expire Minutes
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=30

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/agentarea

# Redis Configuration
REDIS_URL=redis://localhost:6379

# Other settings...
DEBUG=false
APP_ENV=development 